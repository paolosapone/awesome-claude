  "tokenColors": [
    {
      "name": "Basic text & variable names",
      "scope": [
        "text",
        "source",
        "variable.other.readwrite",
        "punctuation.definition.variable"
      ],
      "settings": {
        "foreground": "#E4E4E4"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#969696",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#86E89A"
      }
    },
    {
      "name": "String escapes",
      "scope": [
        "constant.character.escape",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#FF85C8"
      }
    },
    {
      "name": "Numbers and constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined",
        "variable.other.constant",
        "entity.name.constant"
      ],
      "settings": {
        "foreground": "#FFDF61"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.word",
        "keyword.operator.new",
        "storage.type",
        "storage.modifier",
        "variable.language.this",
        "variable.language.super",
        "punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#C79BFF",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.terminator",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#5DE4FA"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "foreground": "#FFA65D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Classes and types",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "support.type",
        "entity.name.namespace",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#7AB8FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Interfaces",
      "scope": [
        "entity.name.interface",
        "entity.name.trait"
      ],
      "settings": {
        "foreground": "#7AB8FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Enums",
      "scope": [
        "entity.name.enum"
      ],
      "settings": {
        "foreground": "#7AB8FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Enum members",
      "scope": [
        "variable.other.enummember",
        "constant.other.enum"
      ],
      "settings": {
        "foreground": "#5DE4FA"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#E07A47"
      }
    },
    {
      "name": "Object properties",
      "scope": [
        "variable.other.property",
        "meta.property.object",
        "entity.name.tag.yaml",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#9C9893"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "meta.annotation",
        "punctuation.decorator",
        "punctuation.definition.decorator",
        "storage.type.annotation"
      ],
      "settings": {
        "foreground": "#FF85C8"
      }
    },
    {
      "name": "Regular expressions",
      "scope": [
